function handleSelectCal1(type,args,obj){var dates=args[0];var date=dates[0];var year=date[0],month=date[1],day=date[2];var selDay=document.searchfrm.sDay;var selMonth=document.searchfrm.sMonth;var selYear=document.searchfrm.sYear;for(i=0;i<=selDay.length;i++){if(selDay.options[i]){if(selDay.options[i].value==day){selDay.options[i].selected=true;break;}}}
for(i=0;i<=selYear.length;i++){if(selYear.options[i]){if(selYear.options[i].value==year){selYear.options[i].selected=true;break;}}}
for(i=0;i<=selMonth.length;i++){if(selMonth.options[i]){if(selMonth.options[i].value==month){selMonth.options[i].selected=true;break;}}}
ymbClearCalendar();}
function handleSelectCal2(type,args,obj){var dates=args[0];var date=dates[0];var year=date[0],month=date[1],day=date[2];var selDay=document.searchfrm.eDay;var selMonth=document.searchfrm.eMonth;var selYear=document.searchfrm.eYear;for(i=0;i<=selDay.length;i++){if(selDay.options[i]){if(selDay.options[i].value==day){selDay.options[i].selected=true;break;}}}
for(i=0;i<=selYear.length;i++){if(selYear.options[i]){if(selYear.options[i].value==year){selYear.options[i].selected=true;break;}}}
for(i=0;i<=selMonth.length;i++){if(selMonth.options[i]){if(selMonth.options[i].value==month){selMonth.options[i].selected=true;break;}}}
ymbClearCalendar();}
