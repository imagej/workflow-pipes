function select_radio_this_user() {
    var sfrm = document.searchfrm;
    if ( sfrm.authoredby[2] ) {
    sfrm.authoredby[2].checked = true;
    }
}
function select_radio_myself() {
    var sfrm = document.searchfrm;
    if ( sfrm.authoredby[1] ) {
        sfrm.authoredby[1].checked = true;
    }
}
YUE.addListener('mbsearch_this_user', "click", select_radio_this_user);
YUE.addListener('mbsearch_user_profiles', "click", select_radio_myself);
